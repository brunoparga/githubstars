<table>
  <thead>
    <tr>
      <th>Repository</th>
      <th class="description">Description</th>
      <th>Language</th>
      <th class="tags">Tags</th>
      <% if owner? %>
        <th>Add tag</th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% repos.each do |repo| %>
      <%= render "repo_row", repo: repo %>
    <% end %>
    <%# Fill out the page with empty lines if needed (wireframe)  %>
    <%= render "filler", rows: (13 - repos.length) %>
  </tbody>
</table>
